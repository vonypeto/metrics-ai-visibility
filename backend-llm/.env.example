# Database Configuration
BACKEND_LLM_DATABASE_URI=mongodb+srv://<username>:<password>@cluster0.tfhbu.mongodb.net/dbai
MONGODB_POOL_SIZE=10

# Server Configuration
PORT=3000

# LLM Provider API Keys (at least one required)
# Get OpenAI key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Get Anthropic key from: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Redis Configuration
REDIS_URI=rediss://default:your_redis_password@your_redis_host:your_redis_port